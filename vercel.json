{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/auth/(.*)", "dest": "/server.js" },
    { "src": "/api/property/(.*)", "dest": "/server.js" },
    { "src": "/api/chat/(.*)", "dest": "/server.js" },
    { "src": "/api/message/(.*)", "dest": "/server.js" }
  ]
}
